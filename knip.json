{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/**/*.ts", "src/**/*.tsx", "eslint/**/*.ts"],
  "project": ["src/**/*.ts", "src/**/*.tsx", "eslint/**/*.ts"],
  "ignore": ["**/*.test.ts", "**/*.spec.ts", "scripts/**", "tests/**"],
  "ignoreDependencies": [
    "tailwindcss",
    "tw-animate-css",
    "@hookform/resolvers",
    "react-hook-form",
    "@tanstack/react-query",
    "@tanstack/react-query-devtools",
    "@tanstack/react-table",
    "@eslint/compat",
    "@apidevtools/json-schema-ref-parser",
    "@types/dompurify",
    "@hono/zod-validator"
  ],
  "ignoreBinaries": ["pull_with_retry", "actionlint"],
  "ignoreExportsUsedInFile": true
}
