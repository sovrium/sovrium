{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "eslint/**/*.ts",
    "scripts/**/*.ts",
    "specs/fixtures/**/*.ts"
  ],
  "project": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "eslint/**/*.ts",
    "scripts/**/*.ts",
    "specs/fixtures/**/*.ts"
  ],
  "ignore": ["**/*.test.ts", "**/*.spec.ts", "scripts/tdd-automation/core/pre-validate.ts"],
  "ignoreDependencies": [
    "tailwindcss",
    "tw-animate-css",
    "@hookform/resolvers",
    "react-hook-form",
    "@tanstack/react-query",
    "@tanstack/react-query-devtools",
    "@tanstack/react-table",
    "@eslint/compat",
    "@apidevtools/json-schema-ref-parser",
    "@types/dompurify",
    "@hono/zod-validator",
    "nanoid"
  ],
  "ignoreBinaries": ["pull_with_retry", "actionlint", "gh"],
  "ignoreExportsUsedInFile": true
}
