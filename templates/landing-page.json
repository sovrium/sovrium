  "blocks": [
    {
      "name": "hero-section",
      "type": "div",
      "props": {
        "data-block": "hero-section",
        "className": "text-center max-w-4xl mx-auto"
      },
      "children": [
        {
          "type": "h1",
          "props": {
            "className": "text-5xl font-bold mb-6"
          },
          "content": "$title"
        },
        {
          "type": "p",
          "props": {
            "className": "text-xl text-gray-600 mb-8"
          },
          "content": "$subtitle"
        },
        {
          "type": "div",
          "props": {
            "className": "flex gap-4 justify-center"
          },
          "children": [
            {
              "type": "button",
              "props": {
                "className": "px-6 py-3 rounded-lg transition-colors font-semibold",
                "style": {
                  "backgroundColor": "#3b82f6",
                  "color": "#ffffff"
                }
              },
              "interactions": {
                "hover": {
                  "backgroundColor": "#2563eb"
                }
              },
              "content": "$ctaPrimary"
            },
            {
              "type": "button",
              "props": {
                "className": "px-6 py-3 rounded-lg transition-colors font-semibold",
                "style": {
                  "borderWidth": "2px",
                  "borderStyle": "solid",
                  "borderColor": "#3b82f6",
                  "color": "#3b82f6",
                  "backgroundColor": "transparent"
                }
              },
              "content": "$ctaSecondary"
            }
          ]
        }
      ]
    },
    {
      "name": "feature-card",
      "type": "div",
      "props": {
        "data-block": "feature-card",
        "className": "p-6 border rounded-xl bg-white hover:shadow-lg transition-shadow"
      },
      "children": [
        {
          "type": "div",
          "props": {
            "className": "text-4xl mb-4"
          },
          "content": "$icon"
        },
        {
          "type": "h3",
          "props": {
            "className": "text-xl font-semibold mb-2"
          },
          "content": "$title"
        },
        {
          "type": "p",
          "props": {
            "className": "text-gray-600"
          },
          "content": "$description"
        }
      ]
    },
    {
      "name": "step-card",
      "type": "div",
      "props": {
        "data-block": "step-card",
        "className": "text-center"
      },
      "children": [
        {
          "type": "div",
          "props": {
            "className": "w-16 h-16 rounded-full bg-primary text-white flex items-center justify-center text-2xl font-bold mx-auto mb-4",
            "style": {
              "backgroundColor": "#3b82f6",
              "color": "#ffffff",
              "width": "4rem",
              "height": "4rem",
              "borderRadius": "9999px",
              "display": "flex",
              "alignItems": "center",
              "justifyContent": "center"
            }
          },
          "content": "$number"
        },
        {
          "type": "h3",
          "props": {
            "className": "text-xl font-semibold mb-2"
          },
          "content": "$title"
        },
        {
          "type": "p",
          "props": {
            "className": "text-gray-600"
          },
          "content": "$description"
        }
      ]
    },
    {
      "name": "cta-button",
      "type": "button",
      "props": {
        "data-block": "cta-button",
        "className": "px-8 py-4 bg-white text-primary rounded-xl font-semibold hover:bg-gray-50 transition-colors shadow-lg",
        "style": {
          "backgroundColor": "#ffffff",
          "color": "#3b82f6"
        }
      },
      "content": "$label"
    },
    {
      "name": "language-switcher",
      "type": "language-switcher",
      "props": {
        "variant": "dropdown"
      }
    }
